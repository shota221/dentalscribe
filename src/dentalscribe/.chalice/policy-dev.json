{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Allow",
			"Action": [
				"logs:CreateLogGroup",
				"logs:CreateLogStream",
				"logs:PutLogEvents"
			],
			"Resource": "arn:*:logs:*:*:*"
		},
		{
			"Effect": "Allow",
			"Action": "bedrock:*",
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": "s3:*",
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": "sqs:*",
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": "dynamodb:*",
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"execute-api:ManageConnections"
			],
			"Resource": "arn:aws:execute-api:*:*:*/@connections/*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"transcribe:StartTranscriptionJob",
				"transcribe:GetTranscriptionJob",
				"transcribe:ListTranscriptionJobs",
				"transcribe:DeleteTranscriptionJob"
			],
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": "translate:*",
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": "iam:PassRole",
			"Resource": "arn:aws:iam::438465136655:role/raglab-chat-dev-translate_s3_access_role",
			"Condition": {
				"StringEquals": {
					"iam:PassedToService": "translate.amazonaws.com"
				}
			}
		}
	]
}